<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="News_info">
  <sql id="Base_Column_List">
     rowid,type,title,subtitle,context,is_top,is_top_name,status,is_talk,modification_date,add_date,modification_user,add_user,remark
  </sql>
  <resultMap id="ResultMap" type="com.yichuang.kyjd.app.entity.news_info.News_info">
         <id property="rowid" column="rowid" />
         <id property="type" column="type" />
         <id property="title" column="title" />
         <id property="subtitle" column="subtitle" />
         <id property="context" column="context" />
         <id property="is_top" column="is_top" />
          <id property="is_top_name" column="is_top_name" />
         <id property="status" column="status" />
         <id property="is_talk" column="is_talk" />
         <id property="modification_date" column="modification_date" />
         <id property="add_date" column="add_date" />
         <id property="modification_user" column="modification_user" />
         <id property="add_user" column="add_user" />
         <id property="remark" column="remark" />
  </resultMap>
  <insert id="save" parameterType="com.yichuang.kyjd.app.entity.news_info.News_info">
  <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
  	SELECT LAST_INSERT_ID() AS id 
  </selectKey>
  insert into ht_news_info (type,title,subtitle,context,is_top,status,is_talk,modification_date,add_date,modification_user,add_user,remark)
         values (#{type},#{title},#{subtitle},#{context},#{is_top},#{status},#{is_talk},now(),now(),#{modification_user},#{add_user},#{remark})
  </insert>
 <delete id="deleteByIds" parameterType="java.util.List">
     delete from ht_news_info where rowid in 
        <foreach collection="array" index="index" item="item" open="("
          separator="," close=")">
           #{item}
        </foreach>
 </delete>
 <update id="update" parameterType="com.yichuang.kyjd.app.entity.news_info.News_info">
    update ht_news_info
         <set>
             <if test="rowid != null">
                   rowid = #{rowid},
             </if>
             <if test="type != null">
                   type = #{type},
             </if>
             <if test="title != null">
                   title = #{title},
             </if>
             <if test="subtitle != null">
                   subtitle = #{subtitle},
             </if>
             <if test="context != null">
                   context = #{context},
             </if>
             <if test="is_top != null">
                   is_top = #{is_top},
             </if>
             <if test="status != null">
                   status = #{status},
             </if>
             <if test="is_talk != null">
                   is_talk = #{is_talk},
             </if>
             <if test="modification_date != null">
                   modification_date = #{modification_date},
             </if>
             <if test="add_date != null">
                   add_date = #{add_date},
             </if>
             <if test="modification_user != null">
                   modification_user = #{modification_user},
             </if>
             <if test="add_user != null">
                   add_user = #{add_user},
             </if>
             <if test="remark != null">
                   remark = #{remark}
             </if>
          </set>
     where rowid = #{rowid}
 </update>
 <select id="getById" parameterType="java.lang.String" resultMap="ResultMap">
     select
       rowid,type,title,subtitle,context,is_top,status,is_talk,modification_date,add_date,modification_user,modification_user,remark,(SELECT
		NAME FROM ht_dictionaries WHERE CODE = is_top) AS is_top_name
        from ht_news_info 
        where rowid = #{rowid}
 </select>
 <select id="getList" parameterType="com.yichuang.kyjd.app.entity.news_info.News_info" resultMap="ResultMap">
       select
        <include refid="Base_Column_List" />
        from ht_news_info 
        <where>
             <if test="name != null and name != ''">
           		title like "%"#{title}"%"
             </if>
        </where>
 </select>
 <select id="getPage" parameterType="com.yichuang.kyjd.app.entity.news_info.News_info" resultMap="ResultMap">
 select
       rowid,type,title,subtitle,context,is_top,status,is_talk,modification_date,add_date,modification_user,modification_user,remark,(SELECT
		NAME FROM ht_dictionaries WHERE CODE = is_top) AS is_top_name
        from ht_news_info 
       <where>
             <if test="name != null and name != ''">
         		  title like "%"#{title}"%"
             </if>
       </where>
     
       LIMIT #{pageStart},#{pageEnd}
 </select>
 <select id="getCount" parameterType="com.yichuang.kyjd.app.entity.news_info.News_info" resultType="int">
       select count(*) from ht_news_info 
         <where>
             <if test="name != null and name != ''">
                title like "%"#{title}"%"
             </if>
         </where>
 </select>
 <select id="selectNews_info" parameterType="com.yichuang.kyjd.app.entity.news_info.News_info" resultType="java.util.HashMap">
       select
        <include refid="Base_Column_List" />
        from ht_news_info 
        <where>
             <if test="name != null and name != ''">
           		title like "%"#{title}"%"
             </if>
        </where>
 </select>
</mapper>