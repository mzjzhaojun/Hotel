<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Restaurant">
  <sql id="Base_Column_List">
     rowid,name,number,type,babychair,iscoupon,mincharge,maxdinernum,status,detail,deposit,remark
  </sql>
  <resultMap id="ResultMap" type="com.yichuang.kyjd.system.entity.restaurant.Restaurant">
         <id property="rowid" column="rowid" />
         <id property="name" column="name" />
         <id property="number" column="number" />
         <id property="type" column="type" />
         <id property="babychair" column="babychair" />
         <id property="iscoupon" column="iscoupon" />
         <id property="mincharge" column="mincharge" />
         <id property="maxdinernum" column="maxdinernum" />
         <id property="status" column="status" />
         <id property="detail" column="detail" />
         <id property="deposit" column="deposit" />
         <id property="remark" column="remark" />
  </resultMap>
  <insert id="save" parameterType="com.yichuang.kyjd.system.entity.restaurant.Restaurant">
  <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
  	SELECT LAST_INSERT_ID() AS id 
  </selectKey>
  insert into ht_restaurant (name,number,type,babychair,iscoupon,mincharge,maxdinernum,status,detail,deposit,remark)
         values (#{name},#{number},#{type},#{babychair},#{iscoupon},#{mincharge},#{maxdinernum},#{status},#{detail},#{deposit},#{remark})
  </insert>
 <delete id="deleteByIds" parameterType="java.util.List">
     delete from ht_restaurant where id in 
        <foreach collection="array" index="index" item="item" open="("
          separator="," close=")">
           #{item}
        </foreach>
 </delete>
 <update id="update" parameterType="com.yichuang.kyjd.system.entity.restaurant.Restaurant">
    update ht_restaurant
         <set>
             <if test="rowid != null">
                   rowid = #{rowid},
             </if>
             <if test="name != null">
                   name = #{name},
             </if>
             <if test="number != null">
                   number = #{number},
             </if>
             <if test="type != null">
                   type = #{type},
             </if>
             <if test="babychair != null">
                   babychair = #{babychair},
             </if>
             <if test="iscoupon != null">
                   iscoupon = #{iscoupon},
             </if>
             <if test="mincharge != null">
                   mincharge = #{mincharge},
             </if>
             <if test="maxdinernum != null">
                   maxdinernum = #{maxdinernum},
             </if>
             <if test="status != null">
                   status = #{status},
             </if>
             <if test="detail != null">
                   detail = #{detail},
             </if>
             <if test="deposit != null">
                   deposit = #{deposit},
             </if>
             <if test="remark != null">
                   remark = #{remark}
             </if>
          </set>
     where id = #{id}
 </update>
 <select id="getById" parameterType="java.lang.String" resultMap="ResultMap">
     select
        <include refid="Base_Column_List" />
        from ht_restaurant 
        where id = #{id}
 </select>
 <select id="getList" parameterType="com.yichuang.kyjd.system.entity.restaurant.Restaurant" resultMap="ResultMap">
       select
        <include refid="Base_Column_List" />
        from ht_restaurant 
        <where>
             <if test="name != null and name != ''">
           		name like "%"#{name}"%"
             </if>
        </where>
 </select>
 <select id="getPage" parameterType="com.yichuang.kyjd.system.entity.restaurant.Restaurant" resultMap="ResultMap">
      select
       <include refid="Base_Column_List" />
        from ht_restaurant 
       <where>
             <if test="name != null and name != ''">
         		  name like "%"#{name}"%"
             </if>
       </where>
       LIMIT #{pageStart},#{pageEnd}
 </select>
 <select id="getCount" parameterType="com.yichuang.kyjd.system.entity.restaurant.Restaurant" resultType="int">
       select count(*) from ht_restaurant 
         <where>
             <if test="name != null and name != ''">
                name like "%"#{name}"%"
             </if>
         </where>
 </select>
 <select id="selectRestaurant" parameterType="com.yichuang.kyjd.system.entity.restaurant.Restaurant" resultType="java.util.HashMap">
       select
        <include refid="Base_Column_List" />
        from ht_restaurant 
        <where>
             <if test="name != null and name != ''">
           		name like "%"#{name}"%"
             </if>
        </where>
 </select>
</mapper>