<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="User_role">
  <sql id="Base_Column_List">
     rowid,user_code,role_code,remarks
  </sql>
  <resultMap id="ResultMap" type="com.yichuang.kyjd.rest.entity.user_role.User_role">
         <id property="rowid" column="rowid" />
         <id property="user_code" column="user_code" />
         <id property="role_code" column="role_code" />
         <id property="remarks" column="remarks" />
  </resultMap>
  <insert id="save" parameterType="com.yichuang.kyjd.rest.entity.user_role.User_role">
  <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
  	SELECT LAST_INSERT_ID() AS id 
  </selectKey>
  insert into ht_user_role (user_code,role_code,remarks)
         values (#{user_code},#{role_code},#{remarks})
  </insert>
 <delete id="deleteByIds" parameterType="java.util.List">
     delete from ht_user_role where id in 
        <foreach collection="array" index="index" item="item" open="("
          separator="," close=")">
           #{item}
        </foreach>
 </delete>
 <update id="update" parameterType="com.yichuang.kyjd.rest.entity.user_role.User_role">
    update ht_user_role
         <set>
             <if test="rowid != null">
                   rowid = #{rowid},
             </if>
             <if test="user_code != null">
                   user_code = #{user_code},
             </if>
             <if test="role_code != null">
                   role_code = #{role_code},
             </if>
             <if test="remarks != null">
                   remarks = #{remarks}
             </if>
          </set>
     where id = #{id}
 </update>
 <select id="getById" parameterType="java.lang.String" resultMap="ResultMap">
     select
        <include refid="Base_Column_List" />
        from ht_user_role 
        where id = #{id}
 </select>
 <select id="getList" parameterType="com.yichuang.kyjd.rest.entity.user_role.User_role" resultMap="ResultMap">
       select
        <include refid="Base_Column_List" />
        from ht_user_role 
        <where>
             <if test="name != null and name != ''">
           		name like "%"#{name}"%"
             </if>
        </where>
 </select>
 <select id="getPage" parameterType="com.yichuang.kyjd.rest.entity.user_role.User_role" resultMap="ResultMap">
      select
       <include refid="Base_Column_List" />
        from ht_user_role 
       <where>
             <if test="name != null and name != ''">
         		  name like "%"#{name}"%"
             </if>
       </where>
       LIMIT #{pageStart},#{pageEnd}
 </select>
 <select id="getCount" parameterType="com.yichuang.kyjd.rest.entity.user_role.User_role" resultType="int">
       select count(*) from ht_user_role 
         <where>
             <if test="name != null and name != ''">
                name like "%"#{name}"%"
             </if>
         </where>
 </select>
 <select id="selectUserRole" parameterType="com.yichuang.kyjd.rest.entity.user_role.User_role" resultMap="ResultMap">
       select
        <include refid="Base_Column_List" />
        from ht_user_role 
        <where>
             <if test="user_code != null and user_code != ''">
           		user_code = #{user_code}
             </if>
        </where>
 </select>
</mapper>