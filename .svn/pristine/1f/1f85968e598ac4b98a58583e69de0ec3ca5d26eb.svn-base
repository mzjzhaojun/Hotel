<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Dictionaries">
  <sql id="Base_Column_List">
     rowid,type,code,name,sys_create_time,remarks
  </sql>
  <resultMap id="ResultMap" type="com.yichuang.kyjd.system.entity.dictionaries.Dictionaries">
         <id property="rowid" column="rowid" />
         <id property="type" column="type" />
         <id property="code" column="code" />
         <id property="name" column="name" />
         <id property="sys_create_time" column="sys_create_time" />
         <id property="remarks" column="remarks" />
  </resultMap>
  <insert id="save" parameterType="com.yichuang.kyjd.system.entity.dictionaries.Dictionaries">
  <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
  	SELECT LAST_INSERT_ID() AS id 
  </selectKey>
  insert into ht_dictionaries (type,code,name,sys_create_time,remarks)
         values (#{type},#{code},#{name},#{sys_create_time},#{remarks})
  </insert>
 <delete id="deleteByIds" parameterType="java.util.List">
     delete from ht_dictionaries where id in 
        <foreach collection="array" index="index" item="item" open="("
          separator="," close=")">
           #{item}
        </foreach>
 </delete>
 <update id="update" parameterType="com.yichuang.kyjd.system.entity.dictionaries.Dictionaries">
    update ht_dictionaries
         <set>
             <if test="rowid != null">
                   rowid = #{rowid},
             </if>
             <if test="type != null">
                   type = #{type},
             </if>
             <if test="code != null">
                   code = #{code},
             </if>
             <if test="name != null">
                   name = #{name},
             </if>
             <if test="sys_create_time != null">
                   sys_create_time = #{sys_create_time},
             </if>
             <if test="remarks != null">
                   remarks = #{remarks}
             </if>
          </set>
     where id = #{id}
 </update>
 <select id="getById" parameterType="java.lang.String" resultMap="ResultMap">
     select
        <include refid="Base_Column_List" />
        from ht_dictionaries 
        where id = #{id}
 </select>
 <select id="getList" parameterType="com.yichuang.kyjd.system.entity.dictionaries.Dictionaries" resultMap="ResultMap">
       select
        <include refid="Base_Column_List" />
        from ht_dictionaries 
        <where>
             <if test="name != null and name != ''">
           		name like "%"#{name}"%"
             </if>
        </where>
 </select>
 <select id="getPage" parameterType="com.yichuang.kyjd.system.entity.dictionaries.Dictionaries" resultMap="ResultMap">
      select
       <include refid="Base_Column_List" />
        from ht_dictionaries 
       <where>
             <if test="name != null and name != ''">
         		  name like "%"#{name}"%"
             </if>
       </where>
       LIMIT #{pageStart},#{pageEnd}
 </select>
 <select id="getCount" parameterType="com.yichuang.kyjd.system.entity.dictionaries.Dictionaries" resultType="int">
       select count(*) from ht_dictionaries 
         <where>
             <if test="name != null and name != ''">
                name like "%"#{name}"%"
             </if>
         </where>
 </select>
 <select id="selectDictionaries" parameterType="com.yichuang.kyjd.system.entity.dictionaries.Dictionaries" resultType="java.util.HashMap">
       select
        <include refid="Base_Column_List" />
        from ht_dictionaries 
        <where>
             <if test="name != null and name != ''">
           		name like "%"#{name}"%"
             </if>
        </where>
 </select>
</mapper>