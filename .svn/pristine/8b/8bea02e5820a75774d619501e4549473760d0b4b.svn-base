<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Room">
  <sql id="Base_Column_List">
     rowid,code,name,detail,instructions,count,surplus,price,sellprice,isenabled,iscoupon,floor,provide,breakfast,wifi,badtype,area,remark
  </sql>
  <resultMap id="ResultMap" type="com.yichuang.kyjd.app.entity.room.Room">
         <id property="rowid" column="rowid" />
         <id property="code" column="code" />
         <id property="name" column="name" />
         <id property="detail" column="detail" />
         <id property="instructions" column="instructions" />
         <id property="count" column="count" />
         <id property="surplus" column="surplus" />
         <id property="price" column="price" />
         <id property="sellprice" column="sellprice" />
         <id property="isenabled" column="isenabled" />
         <id property="iscoupon" column="iscoupon" />
         <id property="floor" column="floor" />
         <id property="provide" column="provide" />
         <id property="breakfast" column="breakfast" />
         <id property="wifi" column="wifi" />
         <id property="badtype" column="badtype" />
         <id property="area" column="area" />
         <id property="remark" column="remark" />
  </resultMap>
  <insert id="save" parameterType="com.yichuang.kyjd.app.entity.room.Room">
  <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
  	SELECT LAST_INSERT_ID() AS id 
  </selectKey>
  insert into ht_room (code,name,detail,instructions,count,surplus,price,sellprice,isenabled,iscoupon,floor,provide,breakfast,wifi,badtype,area,remark)
         values (#{code},#{name},#{detail},#{instructions},#{count},#{surplus},#{price},#{sellprice},#{isenabled},#{iscoupon},#{floor},#{provide},#{breakfast},#{wifi},#{badtype},#{area},#{remark})
  </insert>
 <delete id="deleteByIds" parameterType="java.util.List">
     delete from ht_room where id in 
        <foreach collection="array" index="index" item="item" open="("
          separator="," close=")">
           #{item}
        </foreach>
 </delete>
 <update id="update" parameterType="com.yichuang.kyjd.app.entity.room.Room">
    update ht_room
         <set>
             <if test="code != null">
                   code = #{code},
             </if>
             <if test="name != null">
                   name = #{name},
             </if>
             <if test="detail != null">
                   detail = #{detail},
             </if>
             <if test="instructions != null">
                   instructions = #{instructions},
             </if>
             <if test="count != null">
                   count = #{count},
             </if>
             <if test="surplus != null">
                   surplus = #{surplus},
             </if>
             <if test="price != null">
                   price = #{price},
             </if>
             <if test="sellprice != null">
                   sellprice = #{sellprice},
             </if>
             <if test="isenabled != null">
                   isenabled = #{isenabled},
             </if>
             <if test="iscoupon != null">
                   iscoupon = #{iscoupon},
             </if>
             <if test="floor != null">
                   floor = #{floor},
             </if>
             <if test="provide != null">
                   provide = #{provide},
             </if>
             <if test="breakfast != null">
                   breakfast = #{breakfast},
             </if>
             <if test="wifi != null">
                   wifi = #{wifi},
             </if>
             <if test="badtype != null">
                   badtype = #{badtype},
             </if>
             <if test="area != null">
                   area = #{area},
             </if>
             <if test="remark != null">
                   remark = #{remark}
             </if>
          </set>
     where rowid = #{rowid}
 </update>
 <select id="getById" parameterType="java.lang.String" resultMap="ResultMap">
     select
        <include refid="Base_Column_List" />
        from ht_room 
        where rowid = #{rowid}
 </select>
 <select id="getList" parameterType="com.yichuang.kyjd.app.entity.room.Room" resultMap="ResultMap">
       select
        <include refid="Base_Column_List" />
        from ht_room 
        <where>
             <if test="name != null and name != ''">
           		name like "%"#{name}"%"
             </if>
        </where>
 </select>
 <select id="getPage" parameterType="com.yichuang.kyjd.app.entity.room.Room" resultMap="ResultMap">
      select
       <include refid="Base_Column_List" />
        from ht_room 
       <where>
             <if test="name != null and name != ''">
         		  name like "%"#{name}"%"
             </if>
       </where>
       LIMIT #{pageStart},#{pageEnd}
 </select>
 <select id="getCount" parameterType="com.yichuang.kyjd.app.entity.room.Room" resultType="int">
       select count(*) from ht_room 
         <where>
             <if test="name != null and name != ''">
                name like "%"#{name}"%"
             </if>
         </where>
 </select>
 <select id="selectRoom" parameterType="com.yichuang.kyjd.app.entity.room.Room" resultType="java.util.HashMap">
       select
        <include refid="Base_Column_List" />
        from ht_room 
        <where>
             <if test="name != null and name != ''">
           		name like "%"#{name}"%"
             </if>
        </where>
 </select>
</mapper>